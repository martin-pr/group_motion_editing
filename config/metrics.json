{
	"interactions": [
		{"type": "output_trajectory", "render": "spline", "style": "fill:none;stroke:#aaa;stroke-width:0.5"},
		{"type": "output_trajectory", "render": "circle", "r": 1, "style": "fill:black;stroke:none"},
		{"type": "metrics/distance",
			"ramp": {
				"0": {"r": 1, "g": 0, "b": 0, "a": 1},
				"30": {"r": 1, "g": 0, "b": 0, "a": 0}
			},
			"style": "fill:none;stroke-width:2"}
	],

	"speed": [
		{"type": "metrics/speed",
			"ramp": {
				"0": {"r": 0, "g": 0, "b": 1, "a": 1},
				"1": {"r": 0.95, "g": 0.95, "b": 0.95, "a": 1},
				"2": {"r": 1, "g": 0, "b": 0, "a": 1}
			},
			"style": "fill:none;stroke-width:2"},
		{"type": "output_trajectory", "render": "circle", "r": 1, "style": "fill:black;stroke:none"}
	]
}
